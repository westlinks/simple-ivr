{
    "name": "talgatbaltasov/simple-ivr",
    "description": "Simple IVR package",
    "keywords": [
        "talgatbaltasov",
        "simple-ivr"
    ],
    "homepage": "https://github.com/talgatbaltasov/simple-ivr",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Talgat Baltassov",
            "email": "Talgat.baltasov@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0|^8.1",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Talgatbaltasov\\SimpleIvr\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Talgatbaltasov\\SimpleIvr\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Talgatbaltasov\\SimpleIvr\\SimpleIvrServiceProvider"
            ],
            "aliases": {
                "SimpleIvr": "Talgatbaltasov\\SimpleIvr\\SimpleIvrFacade"
            }
        }
    }
}
